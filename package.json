{
  "name": "rrdelaney.github.io",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "chalk": "^1.1.3",
    "gh-pages": "^1.0.0",
    "prettier": "^1.4.2",
    "react-scripts": "0.9.5",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "glamor": "^2.20.25",
    "glamorous": "^3.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-emojione": "^3.1.7",
    "react-parallax": "^1.2.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prerender": "NODE_ENV=development babel-node --presets=react-app prerender.js",
    "deploy": "gh-pages -d build -b master",
    "lint": "prettier --no-semi --single-quote -l \"src/**/*.js\"",
    "format": "prettier --no-semi --single-quote --write \"src/**/*.js\""
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "localStorage",
      "fetch"
    ]
  }
